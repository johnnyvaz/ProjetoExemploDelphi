CREATE TABLE CLIENTE (
    ID          INTEGER NOT NULL,
    NOME        VARCHAR(50),
    LOGRADOURO  VARCHAR(50),
    NUMERO      INTEGER,
    BAIRRO      VARCHAR(50),
    CIDADE      VARCHAR(50),
    ESTADO      VARCHAR(50),
    CEP         VARCHAR(8),
    TELEFONE    VARCHAR(10),
    CELULAR     VARCHAR(50),
    EMAIL       VARCHAR(100) CHARACTER SET NONE
);
ALTER TABLE CLIENTE ADD CONSTRAINT PK_CLIENTE PRIMARY KEY (ID);